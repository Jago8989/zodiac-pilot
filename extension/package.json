{
  "name": "zodiac-pilot",
  "version": "1.0.0",
  "description": "Chrome extension to simulate dApp interactions and record transactions",
  "main": "index.js",
  "repository": "https://github.com/gnosis/zodiac-pilot.git",
  "author": "Jan-Felix <jan-felix.schwarz@gnosis.io>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "rimraf public/build",
    "build": "NODE_ENV=production node esbuild.js",
    "zip": "mv public zodiac-pilot && zip -vr zodiac-pilot.zip zodiac-pilot/ -x \"*.DS_Store\" && mv zodiac-pilot public",
    "dev": "NODE_ENV=development node esbuild.js",
    "generate:types": "rimraf ./src/types/typechain && typechain --target=ethers-v5 --out-dir ./src/types/typechain ./src/abi/*.json",
    "test": "jest",
    "check": "yarn check:ts && yarn check:prettier && yarn check:lint && yarn check:spelling",
    "check:ts": "tsc --noEmit",
    "check:lint": "eslint src --ext .ts,.tsx",
    "check:prettier": "prettier \"src/**/*.(ts|tsx)\" --list-different",
    "check:spelling": "cspell \"**\"",
    "fix": "yarn fix:prettier && yarn fix:lint",
    "fix:prettier": "prettier \"src/**/*.(ts|tsx)\" --write",
    "fix:lint": "eslint src --ext .ts --fix"
  },
  "devDependencies": {
    "@gnosis.pm/zodiac": "^2.0.1",
    "@safe-global/safe-core-sdk": "^3.2.0",
    "@safe-global/safe-core-sdk-types": "^1.7.0",
    "@safe-global/safe-ethers-lib": "^1.7.0",
    "@safe-global/safe-service-client": "^1.4.0",
    "@shazow/whatsabi": "^0.2.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@typechain/ethers-v5": "^10.0.0",
    "@types/chrome": "^0.0.193",
    "@types/events": "^3.0.0",
    "@types/jest": "^27.0.3",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-modal": "^3.13.1",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@walletconnect/ethereum-provider": "^1.8.0",
    "classnames": "^2.3.1",
    "copy-to-clipboard": "^3.3.1",
    "cspell": "^5.13.0",
    "dotenv": "^16.0.1",
    "esbuild": "^0.16.17",
    "esbuild-css-modules-plugin": "^2.7.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "ethereum-blockies-base64": "^1.0.2",
    "ethers": "^5.7.2",
    "ethers-proxies": "^1.0.0",
    "events": "^3.3.0",
    "ganache": "^7.0.0-beta.2",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^27.4.5",
    "nanoid": "^3.1.30",
    "node-stdlib-browser": "^1.2.0",
    "prettier": "^2.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.3.1",
    "react-modal": "^3.15.1",
    "react-multisend": "^1.2.0",
    "react-select": "^5.2.1",
    "react-toastify": "^9.0.8",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.2",
    "typechain": "^8.0.0",
    "typescript": "^4.9.4",
    "typescript-plugin-css-modules": "^3.4.0"
  }
}
